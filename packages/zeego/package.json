{
  "name": "@fobos531/zeego",
  "version": "0.6.0",
  "description": "Logical UI primitives, made for screens.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "src/",
  "source": "src/",
  "types": "./index.d.ts",
  "files": [
    "src",
    "lib",
    "build",
    "android",
    "ios",
    "cpp",
    "zeeg.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!examples",
    "index.d.ts",
    "context-menu",
    "dropdown-menu"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "expo-prep": "yarn expo-module prepare"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/nandorojo/zeego",
  "author": "Fernando Rojo <frojo@sas.upenn.edu> (https://twitter.com/fernandotherojo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nandorojo/zeego/issues"
  },
  "homepage": "https://github.com/nandorojo/zeeg#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@radix-ui/react-context-menu": "^0.1.6",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-menu/menu": "*",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^17.0.1",
    "@types/react-native": "0.64.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "expo-module-scripts": "2.0.0",
    "husky": "^6.0.0",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.2.2",
    "typescript": "^4.4"
  },
  "peerDependencies": {
    "@radix-ui/react-context-menu": "*",
    "@react-native-menu/menu": "*",
    "react": "*",
    "react-native": "*",
    "react-native-ios-context-menu": "^1.14.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "gitHead": "4698ab57ca2d6f112d38218bc0fb14b833b77a77",
  "sideEffects": false,
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "0.1.6"
  }
}
